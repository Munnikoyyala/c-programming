#program to find n natural number with , separator

input=5
output=1,2,3,4,5

#include<stdio.h>
void main()
{
    int i,n;
    scanf("%d",&n);
    i=1;
    while(i<=n)
    {
        if(i==1){
            printf("%d",i);
        }
        else{
            printf(",%d",i);
        }
        i++;
    }
}
